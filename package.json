    "build-server": "webpack --config webpack/config/webpack.config.server.js --mode=development",
    "run-server": "node ./server/dist/server.js",
    "build-client:dev": "webpack --config webpack/webpack.config.js",
    "build-client:prod": "webpack --config webpack/webpack.config.js --env=production",
    "test": "jest --no-cache --config ./jest/jest.config.js || exit 0",
    "test-watch": "jest --watch --notify --config ./jest/jest.config.js",
    "test-cover": "jest --coverage --config ./jest/jest.config.js",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test start-dev-server-silent 3000 cy:open",
    "lint": "eslint */**/*.{ts,tsx} --fix",
    "storybook": "start-storybook -p 6006",
